<%= form_with theme: 'modal', model: @production_provide, url: { action: 'create' } do |f| %>
  <%= render 'modal_header' %>
  <section class="modal-card-body">
    <%= render 'error_messages', target: f.object  %>
    <%= render partial: 'production_search', locals: { f: f } %>
  </section>
  <%= f.submit %>
<% end %>
