<td>
  <div class="is-several">
    <%= image_tag model.logo, class: 'image is-24x24' if model.logo.attached? %>
    <span><%= model.title %></span>
  </div>
</td>
<td>
  <p>
    <span><%= model.book_start_days %></span>
    <time><%= model.book_start_at.to_s(:time) %></time>
  </p>
  <p>
    <span><%= model.book_finish_days %></span>
    <time><%= model.book_finish_at.to_s(:time) %></time>
  </p>
</td>
<td>
  <p><time><%= model.deliver_start_at.to_s(:time) %></time></p>
  <p><time><%= model.deliver_finish_at.to_s(:time) %></time></p>
</td>
<td>
  <p>
  <% if model.specialty %>
    <i class="fa-light fa-check has-text-success"></i>
  <% else %>
    <i class="fa-light fa-times has-text-danger"></i>
  <% end %>
  </p>
  <p>
  <% if model.automatic %>
    <i class="fa-light fa-check has-text-success"></i>
  <% else %>
    <i class="fa-light fa-times has-text-danger"></i>
  <% end %>
  </p>
</td>
