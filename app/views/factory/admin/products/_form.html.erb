<%= form_with model: [:admin, @product], local: true do |f| %>
  <%= render 'shared/error_messages', target: @product %>
  <%= render partial: 'nodes/outer_select', locals: { f: f, outer: 'product_taxon' } %>
  <%= f.text_field :name %>
  <%= f.collection_select :part_ids, @parts, :id, :name, {  }, { multiple: true } %>
  <%= f.text_field :qr_prefix %>
  <%= f.number_field :reference_price %>
  <%= f.file_field :logo %>
  <%= f.file_field :main_image %>
  <%= f.text_area :description %>
  <%= f.submit %>
<% end %>
