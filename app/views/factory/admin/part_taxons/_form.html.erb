<%= form_with model: [:admin, @part_taxon], local: true do |f| %>
  <%= render 'shared/error_messages', target: @part_taxon %>
  <%= f.text_field :name %>
  <%= f.number_field :position %>
  <%= render partial: 'nodes/children_select', locals: { f: f } %>
  <%= f.check_box :individual %>
  <%= f.submit %>
<% end %>
