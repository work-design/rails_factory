<%= form_with model: [:admin, @product_taxon] do |f| %>
  <%= render 'shared/error_messages', target: @product_taxon %>
  <%= render partial: 'taxon/nodes/siblings_select', locals: { f: f } %>
  <%= f.text_field :name %>
  <%= f.text_field :profit_margin %>
  <%= f.file_field :logo %>
  <%= f.submit %>
<% end %>
