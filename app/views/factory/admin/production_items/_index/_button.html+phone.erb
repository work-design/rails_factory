<%= form_with theme: nil, model: @production_item, url: { action: 'create' }, class: 'button' do |f| %>
  <%= f.hidden_field :code %>
  <%= f.hidden_field :produced_at, value: Time.current.to_fs(:local) %>
  <%= f.submit '新增' %>
<% end %>
<%= form_with theme: nil, model: @production_item, url: { action: 'batch' }, class: 'button' do |f| %>
  <%= f.submit '批量新增' %>
<% end %>
