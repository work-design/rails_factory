<%= form_with theme: nil, model: @production_item, url: { action: 'create' }, class: 'weui-btn_cell weui-btn_cell-primary is-bordered' do |f| %>
  <%= f.hidden_field :code %>
  <%= f.hidden_field :produced_at, value: Time.current.to_s(:local) %>
  <%= f.submit '新增' %>
<% end %>
<%= form_with theme: nil, model: @production_item, url: { action: 'batch' }, class: 'weui-btn_cell weui-btn_cell-primary is-bordered' do |f| %>
  <%= f.submit '批量新增' %>
<% end %>
