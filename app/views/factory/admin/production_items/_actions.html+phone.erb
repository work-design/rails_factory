<div
  data-controller="mini-program"
  data-mini-program-url-value="/pages/print/execute/index"
  data-mini-program-data-value="<%= { url: url_for(action: 'print_data', id: model.id, only_path: false) }.to_json %>"
>
<%= button_to({ action: 'print', id: model.id }, method: :patch, form: { data: { action: 'click->mini-program#link' }, class: 'weui-actionsheet__cell' }) do %>
  <i class="fa-light fa-print"></i>
  <span><%= t('factory.admin.production_items.print.title') %></span>
<% end %>
</div>
<%= link_to({ action: 'edit', id: model.id }, class: 'weui-actionsheet__cell') do %>
  <i class="fa-light fa-pencil"></i>
  <span><%= t('.edit') %></span>
<% end %>
<%= button_to({ action: 'destroy', id: model.id }, method: :delete, class: 'has-text-danger', form: { turbo_confirm: t('.confirm'), class: 'weui-actionsheet__cell weui-actionsheet__cell_warn' }) do %>
  <i class="fa-light fa-trash"></i>
  <span><%= t('.destroy') %></span>
<% end %>
