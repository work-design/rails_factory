<%= form_with theme: 'modal', model: @provide, url: { action: 'create' } do |f| %>
  <%= render 'modal_header' %>
  <section class="modal-card-body">
    <%= render 'error_messages', target: f.object  %>
    <%= f.text_field :name %>
    <%= render partial: 'provider_search', locals: { f: f } %>
  </section>
  <%= f.submit %>
<% end %>
