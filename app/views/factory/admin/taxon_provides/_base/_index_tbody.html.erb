<td><%= model.provide.name %></td>
<td>
<% if model.provide.provider %>
  <%= model.provide.provider.name %>
<% end %>
</td>
<td>
  <%= form_with theme: nil, model: model, url: { controller: 'taxon_provides', action: 'refresh', id: model.id }, data: { controller: 'input' } do |f| %>
    <%= f.check_box :default, data: { input_target: 'checkbox' }, id: nil, class: 'switch is-rounded' %>
    <label data-action="click->input#check"></label>
  <% end %>
</td>
<td><time data-controller="time"><%= model.created_at.to_fs(:iso8601) %></time></td>
