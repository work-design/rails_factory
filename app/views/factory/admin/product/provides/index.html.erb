<%= render 'product_bar' %>
<%= render 'index_bar' %>
<%= render 'index_actions' %>

<%= render layout: 'index_table' do %>
  <tbody id="tbody">
    <%= render partial: 'index_tbody', layout: 'index_tr', collection: @product_provides, as: :model %>
  </tbody>
  <tbody>
    <%= render partial: 'provide_tbody', layout: 'provide_tr', collection: @provides, as: :model %>
  </tbody>
<% end %>

<%= paginate @provides %>
