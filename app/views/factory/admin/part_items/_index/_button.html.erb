<%= form_with theme: nil, model: @part_item, url: { action: 'create' } do |f| %>
  <%= f.hidden_field :qr_code %>
  <%= f.hidden_field :produced_at, value: Time.current.to_s(:local) %>
  <%= f.submit '新增', class: 'button is-info' %>
<% end %>
<%= form_with theme: nil, model: @production_item, url: { action: 'batch' }, class: 'ml-2' do |f| %>
  <%= f.submit '批量新增', class: 'button is-info' %>
<% end %>
