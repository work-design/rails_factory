<%= form_with theme: 'search', model: Factory::PartItem.new do |f| %>
  <div class="field-body">
    <%= f.date_field 'received_at-gte' %>
    <%= f.date_field 'received_at-lte' %>
    <div class="field is-narrow">
      <%= f.submit %>
      <%= link_to t('.clear'), filter_params(except: ['received_at-gte', 'received_at-lte']), class: 'button is-light' %>
    </div>
  </div>
<% end %>
