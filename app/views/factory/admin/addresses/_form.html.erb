<%= form_with model: @address do |f| %>
  <%= render 'shared/error_messages', target: @address %>
  <%= f.text_field :area %>
  <%= f.text_field :detail %>
  <%= f.text_field :contact %>
  <%= f.text_field :tel %>
  <%= f.text_field :post_code %>
  <%= f.text_field :source %>
  <%= f.submit %>
<% end %>
