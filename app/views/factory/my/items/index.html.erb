<div class="box is-padding-less mx-3">
  <div class="weui-panel__hd">
    <%= @custom.title %>
  </div>
  <div class="weui-panel__bd">
    <div class="weui-cells">
      <% @trade_items.each do |trade_item| %>
        <div class="media">
          <div class="media-left"><%= image_tag trade_item.user.avatar_url, style: 'width: 20px;' %></div>
          <div class="media-content">
            <%= trade_item.user.account_identities.join(',') %>
          </div>
          <div class="media-right">
            <div class="weui-btn weui-btn_mini weui-btn_primary">xx</div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>

