<div class="weui-panel__hd"><%= t('.title') %></div>
<div class="weui-grids">
  <% @produce_plans.each do |produce_plan| %>
    <%= link_to({ controller: 'products', produce_plan_id: produce_plan.id }, class: 'weui-grid') do %>
      <div class="weui-grid__icon"><%= image_tag produce_plan.scene.logo if produce_plan.scene.logo.attached? %></div>
      <p class="weui-grid__label"><%= produce_plan.scene.title %></p>
    <% end %>
  <% end %>
</div>
