<% @produce_plans.each do |produce_on, produce_plans| %>
<div class="weui-panel">
  <div class="weui-panel__hd">
    <span>今天</span>
    <span><%= produce_on %></span>
    <span><%= produce_on.to_s(:week) %></span>
  </div>
  <div class="weui-panel__bd">
    <div class="columns is-flex m-0">
      <%= render partial: 'produce_plan', collection: produce_plans, as: :model %>
    </div>
  </div>
</div>
<% end %>
