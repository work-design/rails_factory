<div data-controller="weui-dialog" id="dialog" style="display: none; opacity: 0">
  <div class="weui-half-screen-dialog" data-weui-dialog-target="dialog">
    <div class="weui-half-screen-dialog__hd">
      <div class="weui-half-screen-dialog__hd__side">
        <%= link_to({ action: 'index' }, class: 'weui-icon-btn') do %>
          <i class="weui-icon-back-circle has-text-info"></i>
        <% end %>
      </div>
      <div class="weui-half-screen-dialog__hd__main">
        <div class="weui-half-screen-dialog__title"><%= t('factory.produce_plans.index.title') %></div>
      </div>
      <div class="weui-half-screen-dialog__hd__side">
        <button class="weui-icon-btn" data-action="weui-dialog#close">
          <i class="weui-icon-close-thin"></i>
        </button>
      </div>
    </div>
    <div class="weui-half-screen-dialog__bd">
      <div class="columns as-border is-flex m-0 p-4">
        <%= render partial: 'factory/produce_plans/produce_plan', collection: @produce_plans, as: :model %>
      </div>
    </div>
    <div class="weui-half-screen-dialog__ft"></div>
  </div>
</div>
